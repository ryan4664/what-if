    "@types/jest": "^27.0.2",
    "@types/uuid": "^8.3.1",
    "apollo-server": "^3.3.0",
    "graphql": "^15.6.1",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "ts-jest": "^27.0.5",
    "uuid": "^8.3.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
